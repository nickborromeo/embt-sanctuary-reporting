
<div>
	<div id="paginator">
		<span class='pages'><%= paginate @licenses, :remote => true %></span>
		<span class='page_number'>Total Records:  <%= @licenses.total_count %></span>
	</div>
	<br/><br/>
	<div id="license-pagination">
     <table id="license_table">
     	<thead id='licenses-head'>
        	<tr>
            	<th>Name</th>
	            <th width='30'>Serial Number</th>
	            <th>E-Mail</th>
	            <th>Account Name</th>
	            <th>License</th>
	            <th>Product Category</th>
	            <th>Version</th>
	            <th>PID</th>
	            <th>Country</th>
	            <th>Registered</th>    
            </tr>
        </thead>
 
      <tbody id="licenses-body">    
      	<% @licenses.each do |lic|%>
			<tr>
            	 <td><%= lic[0] %></td>
                 <td width='30'><%= lic[1] %></td>
                 <td><%= lic[2] %></td>
                 <td><%= lic[3] %></td>
                 <td><%= lic[4] %></td>
                 <td><%= lic[5] %></td>
                 <td><%= lic[6] %></td>
                 <td><%= lic[7] %></td>
                 <td><%= lic[8] %></td>
                 <td><%= lic[9] %></td>
           	</tr>
      	<% end %>
      <tbody>
     </table>
	</div>
	
</div>